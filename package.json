{
  "name": "aaw",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "prisma": {
    "seed": "tsx prisma/seeds/run.ts",
    "schema": "packages/prisma/src/schema.prisma"
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "docker:up": "docker-compose --env-file .env -f docker/@aaw/compose.yml up -d",
    "docker:down": "docker-compose --env-file .env -f docker/@aaw/compose.yml down"
  },
  "devDependencies": {
    "prettier": "latest",
    "tsx": "^3.12.3",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.22.0"
}